<!DOCTYPE html>
<html>
  <head>
    <title>TicketApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'layouts/nav' %>
    <p id="notice">
      <%= flash[:notice] %>
    </p>
    <p id="alert">
      <%= flash[:alert] %>
    </p>
    <%= yield %>
  </body>
</html>
